@{    Layout = null;}<!DOCTYPE html><html><head>    <title>Dynamic Tables</title>    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>    <script>        $(document).ready(function () {            $(".table-container div").not(":first").hide(); // Hide all tables except the first one on page load            $(".btn-show-table").click(function () {                var tableId = $(this).attr("data-table"); // Get the data-table attribute value                $(".table-container div").hide(); // Hide all tables                $("#" + tableId).show(); // Show the selected table            });        });    </script></head><body>    <div>        <button class="btn-show-table" data-table="table1">Show Table 1</button>        <button class="btn-show-table" data-table="table2">Show Table 2</button>        <button class="btn-show-table" data-table="table3">Show Table 3</button>        <button class="btn-show-table" data-table="table4">Show Table 4</button>        <button class="btn-show-table" data-table="table5">Show Table 5</button>    </div>    <div class="table-container">        <div id="table1">            <p>Table 1 content here</p>            <!-- Actual table structure goes here -->        </div>        <div id="table2" style="display:none;">            <p>Table 2 content here</p>        </div>        <div id="table3" style="display:none;">            <p>Table 3 content here</p>        </div>        <div id="table4" style="display:none;">            <p>Table 4 content here</p>        </div>        <div id="table5" style="display:none;">            <p>Table 5 content here</p>        </div>    </div></body></html>